{
  "name": "allure-framework/allure-php-commons",
  "keywords": [
    "php",
    "report",
    "testing",
    "allure",
    "commons"
  ],
  "description": "Allure PHP commons",
  "homepage": "http://allure.qatools.ru/",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Dmitry Baev",
      "email": "baev.dm@gmail.com",
      "role": "Developer"
    },
    {
      "name": "Edward Surov",
      "email": "zoohie@gmail.com",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "allure@qameta.io",
    "source": "https://github.com/allure-framework/allure-php-commons"
  },
  "require": {
    "php": "^8",
    "ext-json": "*",
    "doctrine/annotations": "^1.10.4",
    "jms/serializer": "^3",
    "psr/log": "^1",
    "ramsey/uuid": "^3 || ^4",
    "symfony/mime": "^4.3 || ^5"
  },
  "require-dev": {
    "phpunit/phpunit": "^9",
    "squizlabs/php_codesniffer": "^3"
  },
  "conflict": {
    "doctrine/annotations": "<1.10.4"
  },
  "autoload": {
    "psr-4": {
      "Qameta\\Allure\\": [
        "src",
        "src/Qameta/Allure"
      ],
      "Yandex\\Allure\\Adapter\\": [
        "src/Legacy",
        "src/Yandex/Allure/Adapter"
      ]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Qameta\\Allure\\": "test/Qameta/Allure",
      "Yandex\\Allure\\Adapter\\": "test/Yandex/Allure/Adapter"
    }
  },
  "scripts": {
    "test-cs": "vendor/bin/phpcs -sp",
    "test": "vendor/bin/phpunit",
    "test-legacy": "vendor/bin/phpunit --configuration=phpunit.legacy.xml"
  }
}
