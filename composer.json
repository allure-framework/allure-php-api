{
  "name": "allure-framework/allure-php-commons",
  "keywords": [
    "php",
    "report",
    "testing",
    "allure",
    "commons"
  ],
  "description": "Allure PHP commons",
  "homepage": "http://allure.qatools.ru/",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Dmitry Baev",
      "email": "baev.dm@gmail.com",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "allure@qameta.io",
    "source": "https://github.com/allure-framework/allure-php-commons"
  },
  "require": {
    "php": ">=7.1.3",
    "jms/serializer": "^1 | ^2 | ^3",
    "ramsey/uuid": "^3 | ^4",
    "symfony/mime": "^4.3 | ^5"
  },
  "require-dev": {
    "phpunit/phpunit": "^7 | ^8 | ^9"
  },
  "autoload": {
    "psr-4": {
      "Qameta\\Allure\\": "src/Qameta/Allure",
      "Yandex\\Allure\\Adapter\\": "src/Yandex/Allure/Adapter"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Qameta\\Allure\\": "test/Qameta/Allure",
      "Yandex\\Allure\\Adapter\\": "test/Yandex/Allure/Adapter"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-legacy": "vendor/bin/phpunit --configuration=phpunit.legacy.xml"
  }
}
